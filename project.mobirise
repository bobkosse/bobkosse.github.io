ATH@/complexm5/components/assets/background.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\" selected>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.4\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"hidden\" name=\"usedRecaptcha\" value=\"false\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"(overlay && bg.type=='image')\"></div>\n    <div class=\"container\">\n        <div class=\"row justify-content-center\">\n            <div class=\"col-lg-6\">\n\n                <h2 class=\"mbr-section-title align-left mbr-fonts-style mb-5\" data-app-selector=\".mbr-section-title\" mbr-theme-style=\"display-1\" mbr-if=\"showTitle\">Kom in <b>contact</b></h2>\n                <p class=\"mbr-text mbr-fonts-style align-left\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-text\">Finding<b>Purpose</b><span class=\"dot\">.</span><br>Binnenhaven 1<br>6709PD Wageningen<br><br>Kvk nr.:&nbsp; &nbsp; <a href=\"https://www.kvk.nl/orderstraat/product-kiezen/?kvknummer=822628290000\" class=\"text-primary\" target=\"_blank\">82262829</a><br>BTW nr.:&nbsp; NL862400429B01<br>Bank:&nbsp; &nbsp; &nbsp; NL05RABO0366771515<br></p>\n                <p class=\"contacts mbr-fonts-style pt-5 pb-3 align-left\" mbr-theme-style=\"display-5\" data-app-selector=\".contacts\"><b>\n                        <a href=\"tel:+31646013949\" class=\"text-primary\">+31646 013 949</a><br><a href=\"mailto:info@findingpurpose.nl\" class=\"text-primary\">info@findingpurpose.nl</a></b></p>\n\n            </div>\n            <div mbr-form class=\"col-lg-6 mx-auto mbr-form\">\n                <!--Formbuilder Form-->\n                <form action=\"info@findingpurpose.nl\" name=\"Form Name\" method=\"POST\" class=\"mbr-form form-with-styler\" data-verified=\"info@findingpurpose.nl\">\n                    <div class=\"row\">\n                        <div hidden=\"hidden\" data-form-alert class=\"alert alert-success col-12\">Gaaf dat je contact met ons gezocht hebt! We gaan direct voor je aan de slag!</div>\n                        <div hidden=\"hidden\" data-form-alert-danger class=\"alert alert-danger col-12\">Oops...! some\n                            problem!</div>\n                    </div>\n                    <div class=\"dragArea row\">\n                        <div class=\"col-lg-12\">\n                            <h3 mbr-theme-style=\"display-2\" class=\"mbr-form-title align-left mbr-fonts-style mb-4\">\n                                <b>Contact</b></h3>\n                        </div>\n                        <div class=\"col-lg-6 col-md-12 col-sm-12 form-group mb-3\" data-for=\"name\">\n                            <input type=\"text\" name=\"name\" placeholder=\"Jouw naam\" data-form-field=\"name\" mbr-theme-style=\"display-7\" class=\"form-control\" value>\n                        </div>\n                        <div data-for=\"email\" class=\"col-lg-6 col-md-12 col-sm-12 form-group mb-3\">\n                            <input type=\"email\" name=\"email\" placeholder=\"Jouw e-mailadres\" data-form-field=\"email\" mbr-theme-style=\"display-7\" class=\"form-control\" value>\n                        </div>\n                        <div data-for=\"message\" class=\"col-lg-12 col-md-12 col-sm-12 form-group mb-3\">\n                            <textarea name=\"message\" placeholder=\"Jouw bericht\" data-form-field=\"message\" mbr-theme-style=\"display-7\" class=\"form-control\"></textarea>\n                        </div>\n                        <div class=\"col-auto\" mbr-buttons=\"true\" mbr-theme-style=\"display-7\" data-toolbar=\"-mbrBtnMove,-mbrLink,-mbrBtnRemove,-mbrBtnAdd\"><a type=\"submit\" class=\"w-100 w-100 w-100 btn btn-black\" data-app-placeholder=\"Type Text\">Verstuur</a></div>\n                    </div>\n                </form>\n                <!--Formbuilder Form-->\n            </div>\n\n        </div>\n    </div>\n</section>",
